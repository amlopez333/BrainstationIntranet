{
    "parser": "babel-eslint",
    "extends" : ["airbnb", "eslint:recommended"],
    "env": {
        "browser": true
    },
    "plugins" : [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-equals-spacing": 0,
        "react/forbid-prop-types": 0,
        "jsx-a11y/img-redundant-alt": 0
    }
}